call pathogen#infect()
call pathogen#helptags()
" use only Spaces
set expandtab
" indent with two spaces
set tabstop=2
set shiftwidth=2
" nativ indent detection
set autoindent
set smartindent
" activate language detection
filetype plugin indent on
" Syntax & Color
syntax on 
set t_Co=256
colorscheme delek
" Show LineNumbers
set number
